#include<stdio.h>
int main()
{
int i,j,k;

    

for(i=1;i<=3;i++)
{
    for(j=3-i;j>=1;j--)
    {
        printf(" ");
    }
    for(k=1;k<=2*i-1;k++)
    {
        printf("*");
    }

printf("\n");
}
for(i=1;i<=2;i++)
{
    for(k=1;k<=1;k++)
    {
        printf(" ");
    }
    for(j=5-2*i;j>=1;j--)
    {
        printf("*");
    }
    printf("\n");
}
}
