#include <stdio.h>
void main()
{
    int i,j,k,l,m,t,q,s;
    for(i=1;i<=1;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%d",j);
        }
        for(k=1;k<=1;k++)
        {
            printf(" ");
        }
        for(l=1;l<=2;l++)
        {
            printf("%d",l);
        }
        for(m=1;m<=i;m++)
        {
            printf("%d",m);
        }
        for(q=1;q<=1;q++)
        {
            printf(" ");
        }
        for(s=1;s<=3;s++)
        {
            printf("%d",s);
        }
        for(t=2;t>=1;t--)
        {
            printf("%d",t);
        }
        
    }
}
