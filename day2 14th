#include <stdio.h>

int main()
{
    int i,j,k,l,m,q,n,s;
    for(i=1;i<=1;i++)
    {
        for(j=4;j>=i;j--)
        {
            printf("*");
            
        }
    }
    printf("\n");
    for(j=1;j<=2;j++)
    {
        for(k=2;k>=j;k--)
        {
            printf(" ");
        }
        for(l=1;l>=j;l--)
        {
            printf("*");
        }
        for(m=1;m<=j;m++)
        {
            printf(" ");
        }
        for(s=2;s<=j;s++)
        {
            printf("*");
        }
        
    }
    printf("\n");
     for(q=1;q<=1;q++)
    {
        for(n=4;n>=q;n--)
        {
            printf("*");
            
        }
    }
    
}
    

    
